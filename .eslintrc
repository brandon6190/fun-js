{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "mocha": true
  },
  "rules": {
    "comma-dangle": ["error", "only-multiline"],
    "no-unused-vars": 0,
    "no-restricted-syntax": 0,
    "no-continue": 0,
    "guard-for-in": 0,
    "no-param-reassign": 0,
    "consistent-return": 0
  }
}
